{
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/appliedblockchain/silent-data-rollup-quickstart",
  "workspaces": [
    "./packages/*"
  ],
  "name": "@appliedblockchain/silentdatarollup-root",
  "version": "0.1.0",
  "description": "Custom providers for Silent Data Rollup compatible with ethers.js and Hardhat plugins",
  "keywords": [
    "ethereum",
    "provider",
    "silentdata",
    "rollup",
    "ethers",
    "hardhat"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npm run build -w @silentdatarollup/core -w @silentdatarollup/ethers-provider -w @silentdatarollup/ethers-provider-fireblocks -w @silentdatarollup/hardhat-plugin -w @silentdatarollup/hardhat-plugin-fireblocks",
    "publish-packages": "npm run build && npm publish --workspaces",
    "test": "jest --projects packages/*"
  },
  "author": "",
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.13",
    "@types/node": "22.6.1",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
